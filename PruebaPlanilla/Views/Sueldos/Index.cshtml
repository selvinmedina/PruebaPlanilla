@model IEnumerable<PruebaPlanilla.Models.tbSueldos>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutBase.cshtml";
}
<div class="col-lg-12">
<div class="ibox float-e-margins">
<div class="ibox-title">
	@Html.ActionLink("Registrar ", "Create", null, new {@class="btn btn-primary pull-right" })
	<h2>Index</h2>
</div>
<div class="ibox-content">
<table class="table table-striped table-bordered dataTable">
<thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.tmon_Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.hsue_Cantidad)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.hsue_SueldoAnterior)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.hsue_UsuarioCrea)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.hsue_FechaCrea)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.hsue_UsuarioModifica)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.hsue_FechaModifica)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tbEmpleados.emp_Razoninactivacion)
        </th>
        <th></th>
    </tr>
</thead>
<tbody>
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.tmon_Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.hsue_Cantidad)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.hsue_SueldoAnterior)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.hsue_UsuarioCrea)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.hsue_FechaCrea)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.hsue_UsuarioModifica)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.hsue_FechaModifica)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tbEmpleados.emp_Razoninactivacion)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.hsue_Id }, new {@class="btn btn-primary btn-xs"})
            @Html.ActionLink("Detalles", "Details", new { id=item.hsue_Id }, new {@class="btn btn-default btn-xs"})
        </td>
    </tr>
}
</tbody>
</table>
</div>
</div>
</div>
