@model IEnumerable<PruebaPlanilla.Models.tbTipoPlanillaDetalleDeduccion>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_LayoutBase.cshtml";
}
<div class="col-lg-12">
<div class="ibox float-e-margins">
<div class="ibox-title">
	@Html.ActionLink("Registrar ", "Create", null, new {@class="btn btn-primary pull-right" })
	<h2>Index</h2>
</div>
<div class="ibox-content">
<table class="table table-striped table-bordered dataTable">
<thead>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.tpdd_UsuarioCrea)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tpdd_FechaCrea)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tpdd_UsuarioModifica)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tpdd_FechaModifica)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tbCatalogoDeDeducciones.cde_DescripcionDeduccion)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.tbCatalogoDePlanillas.cpla_DescripcionPlanilla)
        </th>
        <th></th>
    </tr>
</thead>
<tbody>
@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.tpdd_UsuarioCrea)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tpdd_FechaCrea)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tpdd_UsuarioModifica)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tpdd_FechaModifica)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tbCatalogoDeDeducciones.cde_DescripcionDeduccion)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.tbCatalogoDePlanillas.cpla_DescripcionPlanilla)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.tpdd_IdPlanillaDetDeduccion }, new {@class="btn btn-primary btn-xs"})
            @Html.ActionLink("Detalles", "Details", new { id=item.tpdd_IdPlanillaDetDeduccion }, new {@class="btn btn-default btn-xs"})
        </td>
    </tr>
}
</tbody>
</table>
</div>
</div>
</div>
